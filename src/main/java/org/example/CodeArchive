static ArrayList<String> employees = getEmployees();
public static void fillCollection() {
    employees.add("Bob");
    employees.add("Boba");
    print(employees);
    print(employees.size());

    ArrayList<Integer> numbers = new ArrayList<>();
    for (int i = 1; i < 6; i++) {
        numbers.add(i);
    }
    print(numbers);

}

